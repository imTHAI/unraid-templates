<?xml version="1.0"?>
<Container version="2">
  <Name>4get</Name>
  <Repository>luuul/4get:latest</Repository>
  <Registry>https://hub.docker.com/r/luuul/4get/</Registry>
  <Network>host</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://git.lolcat.ca/lolcat/4get/issues</Support>
  <Project>https://git.lolcat.ca/lolcat/4get</Project>
   <Overview>4get: a proxy search engine that truly respects your privacy.&#13;
&#13;
- Rotating proxies on a per-scraper basis&#13;
- Search filters (mostly missing in SearxNG)&#13;
- Bot protection that actually works when enabled&#13;
- Lightweight interface &#x2014; no JavaScript required&#13;
- Favicon fetcher with caching and image proxy&#13;
- Plus a bunch of other useful features&#13;
&#13;
&#x1F449; In short: the best way to actually browse without compromise.&#13;
&#13;
To customize the banner, replace it with your own image in: /mnt/user/appdata/4get/banner Recommended format: PNG, 400x100 px.</Overview>
  <Category>Tools: Network:Web</Category>
  <WebUI>http://[IP]:80</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/imTHAI/unraid-templates/raw/main/4get.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1728834271</DateInstalled>
  <DonateText>Donate to the project</DonateText>
  <DonateLink>https://4get.ca/donate</DonateLink>
  <Requires/>
  <Config Name="FOURGET_SERVER_NAME" Target="FOURGET_SERVER_NAME" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">4get.home.local</Config>
  <Config Name="FOURGET_BOT_PROTECTION" Target="FOURGET_BOT_PROTECTION" Default="" Mode="" Description="Set FOURGET_BOT_PROTECTION=1 and mount a directory containing captcha files to /mnt/user/4get/captcha" Type="Variable" Display="always" Required="false" Mask="false">0</Config>
  <Config Name="FOURGET_PROTO" Target="FOURGET_PROTO" Default="http|https" Mode="" Description="If https, put the valid certificates&#13;&#10;in the folder /mnt/user/appdata/4get/certs&#13;&#10;(fullchain.pem and privkey.pem)" Type="Variable" Display="always" Required="false" Mask="false">http</Config>
  <Config Name="http" Target="80" Default="80" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">80</Config>
  <Config Name="https" Target="443" Default="443" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">443</Config>
  <Config Name="captcha" Target="/var/www/html/4get/data/captcha" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/4get/captcha/</Config>
  <Config Name="HTTPS_CERT" Target="/etc/4get/certs" Default="" Mode="rw" Description="Needed only for https" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/4get/certs</Config>
  <Config Name="HTTPS_CERT" Target="/var/www/html/4get/banner" Default="" Mode="rw" Description="Needed only for https" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/4get/banner</Config>
</Container>
